[application]

config/name="unfaith_lost"
run/main_scene="res://battle.tscn"
config/icon="res://icon.png"

[autoload]

player="*res://player.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/narrowing_conversion=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/deprecated_keyword=false

[display]

window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor]

convert_text_resources_to_binary_on_export=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[node]

name_casing=2

[rendering]

quality/2d/gles2_use_nvidia_rect_flicker_workaround=true
quality/2d/use_pixel_snap=true
threads/thread_model=0
quality/filters/use_nearest_mipmap_filter=true
quality/voxel_cone_tracing/high_quality=false
environment/default_environment="res://default_env.tres"
